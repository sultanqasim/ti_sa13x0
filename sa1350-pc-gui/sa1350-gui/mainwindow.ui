      <property name="styleSheet">
       <string notr="true">/* The tab widget frame */

QTabWidget::pane
{
    border-right: solid #ddd;
    border-bottom: 1px outset #666;
    border-left: 1px outset #666;
    border-top: 1px outset #666;
}

QTabWidget QWidget#tabHardware, QTabWidget QWidget#tabRfSettings, QTabWidget QWidget#tabTraces, QTabWidget QWidget#tabMarker, QTabWidget QWidget#tabScreen
{
	background-color:#dcdcdc;
	color:#000;
}

QTabBar::tab
{
    border-bottom-right-radius: 6px;
    border-top-right-radius: 6px;
    border-right: 1px outset #666;
    border-bottom: 1px outset #666;
    border-left: 1px #f6f6f6;
    border-top: 1px outset #666;
    padding: 4px;
    background-color: #ddd;
    min-height: 80px;
}

QTabBar::tab:selected
{
    color: #000;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 rgba(220, 220, 220, 255), stop:0.75 rgba(220, 220, 220, 255), stop:0.85 rgba(255, 150, 150, 255), stop:1 rgba(255, 0, 0, 255));
}

QTabBar::tab:!selected
{
    color: #666;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 rgba(30, 30, 30, 255), stop:0.10 rgba(150, 150, 150, 255), stop:0.19 rgba(217, 217, 217, 255), stop:0.20 rgba(252, 252, 252, 255), stop:1 rgba(255, 255, 255, 255));
    margin-right: 6px; /* make non-selected tabs look smaller */
}

QTabBar::tab:!selected:hover
{
    color: red
}
</string>
